{
	"name": "termin-all-or-nothing",
	"displayName": "termin-all-or-nothing",
	"description": "I either want to see a full screen terminal or a full screen editor, nothing in between.",
  "publisher": "groogle",
  "repository": {
    "type": "git",
    "url": "https://github.com/leep-frog/termin-all-or-nothing"
  },
	"version": "1.0.2",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
    "onStartupFinished"
	],
	"browser": "./dist/web/extension.js",
	"contributes": {
		"commands": [
		]
	},
	"scripts": {
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
		"pretest": "npm run compile-web",
		"vscode:prepublish": "npm run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
	},
	"devDependencies": {
		"@types/vscode": "^1.54.0",
		"@types/mocha": "^9.1.1",
		"eslint": "^8.18.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4",
		"@vscode/test-web": "^0.0.26",
		"ts-loader": "^9.3.1",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0",
		"@types/webpack-env": "^1.17.0",
		"assert": "^2.0.0",
		"process": "^0.11.10"
	}
}
